diff -Naur watchdog-5.14.orig/redhat/watchdog.init watchdog-5.14/redhat/watchdog.init
--- watchdog-5.14.orig/redhat/watchdog.init	2013-01-18 04:50:28.000000000 -0700
+++ watchdog-5.14/redhat/watchdog.init	2020-04-03 14:53:24.216476528 -0600
@@ -7,7 +7,11 @@
 #                 Henning P. Schmiedehausen <hps@tanstaafl.de>
 
 # Source function library.
-. /etc/rc.d/init.d/functions
+. /etc/init.d/functions
+
+{
+echo -e "\n============================================================"
+date
 
 [ -x /usr/sbin/watchdog -a -e /etc/watchdog.conf ] || exit 0
 
@@ -22,24 +26,28 @@
 lockfile=/var/lock/subsys/watchdog
 
 start() {
+	echo -e "Current u-boot env:\n"
+	fw_printenv
+	echo
+	echo -n "Starting $prog: "
 
-	echo -n $"Starting $prog: "
 	if [ -n "$(pidofproc $prog)" ]; then
-		echo -n $"$prog: already running"
-		echo_failure
+		echo -n "$prog: already running "
+		failure
 		echo
 		return 1
 	fi
 	if [ "$VERBOSE" = "yes" ]; then
-	    daemon /usr/sbin/${prog} -v
+	    /usr/sbin/${prog} -v
 	else
-	    daemon /usr/sbin/${prog}
+	    /usr/sbin/${prog}
         fi
 	RETVAL=$?
 	[ $RETVAL -eq 0 ] && touch $lockfile
-	[ $RETVAL -eq 0 ] && echo_success
-	[ $RETVAL -ne 0 ] && echo_failure
+	[ $RETVAL -eq 0 ] && success
+	[ $RETVAL -ne 0 ] && failure
 	echo
+
 	return $RETVAL
 }
 
@@ -50,8 +58,10 @@
 	# and reboot the box.
 	killproc $prog -TERM
 	RETVAL=$?
-	echo
 	[ $RETVAL -eq 0 ] && rm -f $lockfile $pidfile
+	[ $RETVAL -eq 0 ] && success
+	[ $RETVAL -ne 0 ] && failure
+	echo
 	return $RETVAL
 }
 
@@ -84,3 +94,5 @@
 	echo $"Usage: $0 {start|stop|restart|status|condrestart}"
 	exit 1
 esac
+
+} >> /media/extra/log/watchdog.log 2>&1
