/dev/root	/			auto	defaults,noatime,nodiratime,barrier=0	1 1
proc		/proc			proc	defaults	0 0
devpts		/dev/pts		devpts	mode=0620,gid=5	0 0
tmpfs		/run			tmpfs	mode=0755,nodev,nosuid,strictatime	0 0
tmpfs		/var/volatile		tmpfs	defaults,size=16M	0 0
tmpfs		/media/ram		tmpfs	defaults,size=16M	0 0
/dev/mmcblk0p1	/media/card		auto	ro,noatime,nodiratime,sync	0 0
/dev/mmcblk2p3	/media/bootscript	auto	ro,noatime,nodiratime,sync	0 0
/dev/mmcblk2p5	/media/mmcblk2p5	auto	rw,noatime,nodiratime,sync	0 0
/dev/mmcblk2p6	/media/mmcblk2p6	auto	rw,noatime,nodiratime,sync	0 0
/dev/mmcblk2p7	/media/extra		auto	rw,noatime,nodiratime,async,barrier=0	0 0

# Loop mount local clips directory to limit size of clips directory
/media/extra/clips.img	/media/clips	auto	loop,noatime,nodiratime,sync,noauto 	0 0

